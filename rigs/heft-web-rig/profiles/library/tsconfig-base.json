{
  "$schema": "http://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "outDir": "../../../../../lib",
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "allowJs": false,
    "declaration": true,
    "sourceMap": true,
    "declarationMap": true,
    "inlineSources": true,
    "experimentalDecorators": false,
    "strict": true,
    "useUnknownInCatchVariables": true,
    "esModuleInterop": true,
    "noEmitOnError": false,
    "allowUnreachableCode": true,
    "importHelpers": false,
    "types": [   ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "moduleDetection": "force",
    "target": "ES2020",
    "lib": [
      "ESNext",
      "DOM",
      "ScriptHost"
    ],
    "skipLibCheck": true,
    "incremental": true,
    "isolatedModules": true
  },
  "exclude": [
    "**/*.test-d.ts"
  ]
}